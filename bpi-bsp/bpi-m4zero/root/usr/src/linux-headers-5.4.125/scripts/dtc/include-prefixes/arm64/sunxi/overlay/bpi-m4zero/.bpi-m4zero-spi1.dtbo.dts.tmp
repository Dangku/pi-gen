# 0 "arch/arm64/boot/dts/sunxi/overlay/bpi-m4zero/bpi-m4zero-spi1.dts"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/sunxi/overlay/bpi-m4zero/bpi-m4zero-spi1.dts"
/dts-v1/;
/plugin/;

/ {
 fragment@0 {
  target = <&spi1>;
  __overlay__ {
   status = "okay";

   spi1_cs_number = <0x2>;
   spi1_cs_bitmap = <0x3>;

   spi_board0@0 {
    device_type = "spi_board0";
    compatible = "linux,spidev";
    spi-max-frequency = <10000000>;
    reg = <0x0>;
    status = "okay";
   };

   spi_board1@1 {
    device_type = "spi_board1";
    compatible = "linux,spidev";
    spi-max-frequency = <10000000>;
    reg = <0x1>;
    status = "okay";
   };
  };
 };
};
